#!/bin/zsh

if [ "$1" = "--help" ]; then
  printf "The `£` command, all you need for \`my\`"
  printf "\n\n"
  printf "usage: £ [<command>]"
  printf "\n\n"
  printf "      bam   start install/update/upgrade"
  printf "\n"
  printf "    --help  give you help"
  printf "\n"
fi

if [ "$1" = "bam" ]; then
  rocknroll-cli
fi
